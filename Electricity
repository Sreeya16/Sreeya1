#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   
    int u;
    scanf("%d",&u);
        if(u<=200)
        {
           int a=0.5*u;
                printf("Rs.%d",a);
        }
    else if(u<=400)
    {
       int b=(0.65*u)+100;
        printf("Rs.%d",b);
    }
    else if(u<=600)
    {
       int c=(0.8*u)+200;
        printf("Rs.%d",c);
    }
    else if(u>600)
    {
       int d=(1.25*u)+425;
        printf("Rs.%d",d);
    }
    else
    {
        printf("Invalid");
    }
    return 0;
}
